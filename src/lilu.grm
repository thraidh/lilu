grammar Lilu;

lilufile ::= ^ block $;
block ::= element*;
element ::= funcdef | expr ;
extern expr;
args ::= [ arg [ "," arg ]* ]?;
arg ::= name=ID ":" type=ID;
funcdef ::= "function" name=ID "(" args ")" ":" rtype=ID "is" body=block "end";
primary ::= id | number | "(" expr ")" ;
id ::= ID;
number ::= NUMBER;

